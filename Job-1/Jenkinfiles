pipeline{
    agent any
    tools {
        maven 'Maven'
        jdk 'Java_11'
    }

    stages{
        stage('Git Checkout'){
            steps{
               git branch: 'main',url:'https://github.com/sharath050175/inetbankingv4.git'
            }
        }

        stage('Maven clean'){
            steps{
                sh 'mvn clean'
            }
        }

        stage('Maven build'){
            steps{
                sh 'mvn Build'
            }
        }

    }
}